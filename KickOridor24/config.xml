<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets"
        xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>KickOridor24</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="front/index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <icon src="www/front/images/icon.png" platform="android" width="57" height="57" density="mdpi" />
    <icon src="www/front/images/icon.png" platform="ios" width="57" height="57" density="mdpi" />
    <preference name="hostname" value="kickoridor.ps8.academy"/>
    <preference name="scheme" value="http"/>
    <preference name="AndroidWindowSplashScreenAnimatedIcon" value="www/front/images/icon.png"/>
    <preference name="AndroidWindowSplashScreenBackground" value="#000000"/>
    <preference name="orientation" value="portrait" />
    <access origin="http://kickoridor.ps8.academy" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <edit-config
            xmlns:android="http://schemas.android.com/apk/res/android"
            file="app/src/main/AndroidManifest.xml"
            mode="merge"
            target="/manifest/application">
        <application android:usesCleartextTraffic="true" />
    </edit-config>
</widget>
